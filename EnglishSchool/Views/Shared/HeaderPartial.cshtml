<header>
    <div id="contacts">
        <span>@Config.CompanyPhone</span>
        <span>@Config.CompanyEmail</span>
        <a style="text-decoration: none" href="@Config.Instagram">@Config.Instagram</a>
        <span>@ViewBag.userTitle</span>
        <a href="https://localhost:44355/admin" class="login">Log in</a>
    </div>
    <div id="logo" onclick="slowScroll('#top')">
        <a style="text-decoration: none" asp-area="" asp-controller="Home" asp-action="Index">@Config.CompanyName</a>
    </div>
    <div id="control">
        <a style="text-decoration: none" asp-area="" asp-controller="Course" asp-action="Index">Курсы</a>
        <a style="text-decoration: none" asp-area="" asp-controller="GalleryUser" asp-action="Index">Галерея</a>
        <a style="text-decoration: none" asp-area="" asp-controller="Timetable" asp-action="Index">Расписание</a>
        <a style="text-decoration: none" asp-area="" asp-controller="About" asp-action="Index">Преподаватель</a>
        <a style="text-decoration: none" asp-area="" asp-controller="Request" asp-action="Index">Заявка</a>
    </div>
</header>
